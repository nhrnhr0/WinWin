{
  "kind": "collectionType",
  "collectionName": "institutions",
  "info": {
    "singularName": "institution",
    "pluralName": "institutions",
    "displayName": "עמותה institution",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "lat": {
      "type": "string"
    },
    "lng": {
      "type": "string"
    },
    "location": {
      "type": "string",
      "pluginOptions": {
        "mapfield": {
          "enabled": true
        }
      }
    },
    "description": {
      "type": "richtext"
    },
    "contact_info": {
      "type": "component",
      "repeatable": false,
      "component": "attributes.contact-information"
    },
    "main_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "opening_housers": {
      "type": "component",
      "repeatable": false,
      "component": "attributes.opening-hours"
    },
    "businesses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::business.business",
      "inversedBy": "institutions"
    },
    "address": {
      "type": "string"
    },
    "frequently_asked_questions": {
      "type": "component",
      "repeatable": true,
      "component": "attributes.qn-a"
    },
    "institution_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::institution-category.institution-category",
      "inversedBy": "institutions"
    }
  }
}
